{
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.test.config": [
        {
        "name": "test",
        "workingDirectory": "${workspaceFolder}/java/metaheuristic/src/test/resources/"
        }
    ],
    "java.configuration.runtimes": [
      {
        "name": "JavaSE-17",
        "path": "${HOME}/.sdkman/candidates/java/17.0.3-oracle/",
        "default": true
      }
    ],
    "java.import.gradle.home": "${HOME}/.sdkman/candidates/gradle/7.4.2/",
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx4G -Xms100m"
}
