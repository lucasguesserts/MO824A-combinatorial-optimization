  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${fileDirname}"
    },
    {
      "type": "java",
      "name": "KQBF",
      "request": "launch",
      "mainClass": "${workspaceFolder}/KQBF/src/main/java/Main.java",
      "cwd": "${workspaceFolder}/KQBF"
    },
    {
      "type": "java",
      "name": "KQBF_GRASP",
      "request": "launch",
      "mainClass": "${workspaceFolder}/KQBF_GRASP/src/main/java/Main.java",
      "cwd": "${workspaceFolder}/KQBF_GRASP"
    }
  ]
}
